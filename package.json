{
  "name": "webhook-tester",
  "version": "1.0.0",
  "description": "A webhook testing tool with real-time logging",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc && cp -r src/public dist/",
    "start": "node dist/server.js",
    "dev": "ts-node src/server.ts",
    "dev:watch": "nodemon --exec ts-node src/server.ts"
  },
  "dependencies": {
    "express": "^4.18.2",
    "socket.io": "^4.7.5",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "@types/cors": "^2.8.17",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.2",
    "nodemon": "^3.0.2"
  },
  "keywords": ["webhook", "testing", "express", "socket.io"],
  "author": "",
  "license": "MIT"
}
